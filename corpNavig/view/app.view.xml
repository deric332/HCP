<mvc:View
displayBlock="true"
    xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout"
	xmlns:al="sap.ui.commons.layout.VAlign"
	xmlns:core="sap.ui.core"
    xmlns="sap.m"
controllerName="MyApp.corpNavig.controller.app">

	<App backgroundImage = "./Accenture.jpg"> 
		<pages>
			<Page title="Coprporate Navigator">
				<content>
					<VBox class="sapUiMediumMargin" alignItems="center">
						<Input value = "{login>/userName}" placeholder=" Username" width = "33%"/>
						<Input value = "{login>/password}" placeholder=" Password" type="Password" width = "33%" />
						<Button text="Log On" type="Default" press="onLogin"/>
					</VBox>
				</content>
				
			</Page>
		</pages>
	</App>


</mvc:View>