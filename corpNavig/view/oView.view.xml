<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout"
	controllerName="MyApp.corpNavig.controller.oView">
	<App id="app" backgroundImage="./img/appBG.jpg">
		<pages>
			<Page id="oView" title="Corporate Navigator" design="Header">
				<headerContent>
					<Button  class="profileImage" id="pImage_oView" icon="{/image}" tooltip="My Profile" press="onProfile"/>
				</headerContent>
				<FlexBox height="100%" alignItems="Center" justifyContent="Center" fitContainer="true" wrap="Wrap">
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout myappcenter" header=" Leadership" press="press">
						<tileContent>
							<TileContent >
								<content>
									<ImageContent src="./img/people.png"/>
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout myappcenter" header=" Innovations" press="press">
						<tileContent>
							<TileContent >
								<content>
									<ImageContent src="./img/innovation.png"/>
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout myappcenter" header=" Financial Reports" press="press">
						<tileContent>
							<TileContent >
								<content>
									<ImageContent src="./img/finance.png"/>
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>
					<!---->
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout myappcenter" header=" Sales Leads" press="press">
						<tileContent>
							<TileContent >
								<content>
									<ImageContent src="./img/Leads.png"/>
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>
					<!---->
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout myappcenter" header=" Sales Opportunities" press="press">
						<tileContent>
							<TileContent >
								<content>
									<ImageContent src="./img/opportunity.png"/>
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>
					<!---->
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout myappcenter " header=" CRM Dashboard" press="onTileCrm">
						<tileContent>
							<TileContent >
								<content>
									<ImageContent src="./img/dashboard.png"/>
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>
				</FlexBox>
			</Page>
		</pages>
	</App>
</mvc:View>